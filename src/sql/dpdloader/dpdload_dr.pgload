      WITH ENCODING UTF-8
      (
        DRUG_CODE,
        ACTIVE_INGREDIENT_CODE,
        INGREDIENT,
        INGREDIENT_SUPPLIED_IND,
        STRENGTH,
        STRENGTH_UNIT,
        STRENGTH_TYPE,
        DOSAGE_VALUE,
        BASE,
        DOSAGE_UNIT,
        NOTES,
        ingredient_f,
        strength_unit_f,
        strength_type_f,
        dosage_unit_f
      )
    INTO postgresql:///?dpd.active_ingredient

      (
        DRUG_CODE,
        ACTIVE_INGREDIENT_CODE,
        INGREDIENT,
        INGREDIENT_SUPPLIED_IND,
        STRENGTH,
        STRENGTH_UNIT,
        STRENGTH_TYPE,
