-- PGloader script for the Health Canada Drug Product Database
-- Connection details drawn from environment variables PGUSER, PGPASSWORD, PGHOST, PGDATABASE
-- Run after dpdload.pgload and before dpdload_ap.pgload

LOAD ARCHIVE
-- FROM https://www.canada.ca/content/dam/hc-sc/documents/services/drug-product-database/allfiles_dr.zip
   FROM /home/nzhu/dpd_20200203/allfiles_dr.zip
  INTO postgresql:///


  BEFORE LOAD DO
    $$ ALTER TABLE dpd.companies ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.drug_product ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.pharmaceutical_form ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.active_ingredient ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.packaging ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.pharmaceutical_std ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.route ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.schedule ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.status ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.therapeutic_class ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.vet_species ALTER COLUMN extract SET DEFAULT 'dormant'; $$,
    $$ ALTER TABLE dpd.status ALTER COLUMN expiration_date SET DATA TYPE text; $$


  LOAD CSV
    FROM FILENAME MATCHING ~/comp_dr.txt/
      WITH ENCODING UTF-8
      (
        DRUG_CODE,
        MFR_CODE,
        COMPANY_CODE,
        COMPANY_NAME,
        COMPANY_TYPE,
        ADDRESS_MAILING_FLAG,
        ADDRESS_BILLING_FLAG,
        ADDRESS_NOTIFICATION_FLAG,
        ADDRESS_OTHER,
        SUITE_NUMBER,
        STREET_NAME,
        CITY_NAME,
        PROVINCE,
        COUNTRY,
        POSTAL_CODE,
        POST_OFFICE_BOX,
        province_f,
        country_f
      )
    INTO postgresql:///?dpd.companies
      (
        DRUG_CODE,
        MFR_CODE,
        COMPANY_CODE,
        COMPANY_NAME,
        COMPANY_TYPE,
        ADDRESS_MAILING_FLAG,
        ADDRESS_BILLING_FLAG,
        ADDRESS_NOTIFICATION_FLAG,
        ADDRESS_OTHER,
        SUITE_NUMBER,
        STREET_NAME,
        CITY_NAME,
        PROVINCE,
        COUNTRY,
        POSTAL_CODE,
        POST_OFFICE_BOX,
        province_f,
        country_f
      )
    WITH fields terminated by ',',
             fields escaped by double-quote



 AND LOAD CSV
    FROM FILENAME MATCHING ~/drug_dr.txt/
      WITH ENCODING UTF-8
      (
        DRUG_CODE,
        PRODUCT_CATEGORIZATION,
        CLASS,
        DRUG_IDENTIFICATION_NUMBER,
        BRAND_NAME,
        DESCRIPTOR,
        PEDIATRIC_FLAG,
        ACCESSION_NUMBER,
        NUMBER_OF_AIS,
        LAST_UPDATE_DATE,
        AI_GROUP_NO,
        class_f,
        brand_name_f,
        descriptor_f
      )
    INTO postgresql:///?dpd.drug_product
      (
        DRUG_CODE,
        PRODUCT_CATEGORIZATION,
        CLASS,
        DRUG_IDENTIFICATION_NUMBER,
        BRAND_NAME,
        DESCRIPTOR,
        PEDIATRIC_FLAG,
        ACCESSION_NUMBER,
        NUMBER_OF_AIS,
        LAST_UPDATE_DATE,
        AI_GROUP_NO,
        class_f,
        brand_name_f,
        descriptor_f
      )
    WITH fields terminated by ',',
             fields escaped by double-quote



AND LOAD CSV
