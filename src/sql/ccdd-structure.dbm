	LEFT JOIN ccdd_dpd_ingredient_ntp_mapping ntpmap ON (ntpmap.dpd_named_ingredient_name = ddio.dpd_named_ingredient_name)
where ntpmap.ccdd_ntp_ingredient_name is null
order by dd.din
]]></expression>
	</reference>
</view>

